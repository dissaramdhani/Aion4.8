<?xml version="1.0" encoding="utf-8"?>
<global_rules xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../global_rules.xsd">
	<!-- Multiple Monster Drops -->
	<gd_rule rule_name="Named Key" chance="100">
		<gd_maps>
			<gd_map map_id="301250000" /><!-- Legion_Miren_Barracks -->
			<gd_map map_id="301290000" /><!-- Miren_Barracks -->
		</gd_maps>
		<gd_npcs>
			<gd_npc npc_id="233713" /><!-- Treasurer Karpasha -->
			<gd_npc npc_id="233714" /><!-- Treasurer Pasikar -->
			<gd_npc npc_id="233715" /><!-- Treasurer Subatita -->
			<gd_npc npc_id="233716" /><!-- Treasurer Mahirwa -->
		</gd_npcs>
		<gd_items>
			<gd_item id="185000066" /><!-- Miren Armory Key -->
			<gd_item id="185000067" /><!-- Miren Supply Base Key -->
			<gd_item id="185000068" /><!-- Miren Operations Room Key -->
		</gd_items>
	</gd_rule>
	<!-- Single Monster Drop -->
	<gd_rule rule_name="Named Mark" chance="100" member_limit="6">
		<gd_maps>
			<gd_map map_id="301250000" /><!-- Legion_Miren_Barracks -->
			<gd_map map_id="301290000" /><!-- Miren_Barracks -->
		</gd_maps>
		<gd_npcs>
			<gd_npc npc_id="233718" /><!-- Weakened Miren Prince -->
			<gd_npc npc_id="233719" /><!-- Enraged Miren Prince -->
		</gd_npcs>
		<gd_items>
			<gd_item id="186000238" min_count="60" max_count="90" /><!-- Conqueror's Mark -->
		</gd_items>
	</gd_rule>
	<gd_rule rule_name="Named Treasure Key" chance="100">
		<gd_maps>
			<gd_map map_id="301250000" /><!-- Legion_Miren_Barracks -->
			<gd_map map_id="301290000" /><!-- Miren_Barracks -->
		</gd_maps>
		<gd_npcs>
			<gd_npc npc_id="233718" /><!-- Weakened Miren Prince -->
			<gd_npc npc_id="233719" /><!-- Enraged Miren Prince -->
		</gd_npcs>
		<gd_items>
			<gd_item id="185000070" /><!-- Miren Gold Room Key -->
		</gd_items>
	</gd_rule>
	<gd_rule rule_name="Named Gold Key" chance="100">
		<gd_maps>
			<gd_map map_id="301250000" /><!-- Legion_Miren_Barracks -->
			<gd_map map_id="301290000" /><!-- Miren_Barracks -->
		</gd_maps>
		<gd_npcs>
			<gd_npc npc_id="233718" /><!-- Weakened Miren Prince -->
			<gd_npc npc_id="233719" /><!-- Enraged Miren Prince -->
		</gd_npcs>
		<gd_items>
			<gd_item id="185000069" /><!-- Miren Treasure Room Key -->
		</gd_items>
	</gd_rule>
</global_rules>