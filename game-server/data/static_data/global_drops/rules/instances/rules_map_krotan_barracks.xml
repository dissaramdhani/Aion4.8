<?xml version="1.0" encoding="utf-8"?>
<global_rules xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../global_rules.xsd">
	<!-- Multiple Monster Drops -->
	<gd_rule rule_name="Named Key" chance="100">
		<gd_maps>
			<gd_map map_id="301260000" /><!-- Legion_Krotan_Barracks -->
			<gd_map map_id="301300000" /><!-- Krotan_Barracks -->
		</gd_maps>
		<gd_npcs>
			<gd_npc npc_id="233627" /><!-- Treasurer Wasukani -->
			<gd_npc npc_id="233628" /><!-- Treasurer Baruna -->
			<gd_npc npc_id="233629" /><!-- Treasurer Matazawa -->
			<gd_npc npc_id="233630" /><!-- Treasurer Hittite -->
		</gd_npcs>
		<gd_items>
			<gd_item id="185000056" /><!-- Krotan Armory Key -->
			<gd_item id="185000057" /><!-- Krotan Supply Base Key -->
			<gd_item id="185000058" /><!-- Krotan Operations Room Key -->
		</gd_items>
	</gd_rule>
	<!-- Single Monster Drops -->
	<gd_rule rule_name="Named Mark" chance="100" member_limit="6">
		<gd_maps>
			<gd_map map_id="301260000" /><!-- Legion_Krotan_Barracks -->
			<gd_map map_id="301300000" /><!-- Krotan_Barracks -->
		</gd_maps>
		<gd_npcs>
			<gd_npc npc_id="233632" /><!-- Weakened Krotan Lord -->
			<gd_npc npc_id="233633" /><!-- Enraged Krotan Lord -->
		</gd_npcs>
		<gd_items>
			<gd_item id="186000238" min_count="60" max_count="90" /><!-- Conqueror's Mark -->
		</gd_items>
	</gd_rule>
	<gd_rule rule_name="Named Treasure Key" chance="100">
		<gd_maps>
			<gd_map map_id="301260000" /><!-- Legion_Krotan_Barracks -->
			<gd_map map_id="301300000" /><!-- Krotan_Barracks -->
		</gd_maps>
		<gd_npcs>
			<gd_npc npc_id="233632" /><!-- Weakened Krotan Lord -->
			<gd_npc npc_id="233633" /><!-- Enraged Krotan Lord -->
		</gd_npcs>
		<gd_items>
			<gd_item id="185000059" /><!-- Krotan Treasure Room Key -->
		</gd_items>
	</gd_rule>
	<gd_rule rule_name="Named Gold Key" chance="100">
		<gd_maps>
			<gd_map map_id="301260000" /><!-- Legion_Krotan_Barracks -->
			<gd_map map_id="301300000" /><!-- Krotan_Barracks -->
		</gd_maps>
		<gd_npcs>
			<gd_npc npc_id="233632" /><!-- Weakened Krotan Lord -->
			<gd_npc npc_id="233633" /><!-- Enraged Krotan Lord -->
		</gd_npcs>
		<gd_items>
			<gd_item id="185000060" /><!-- Krotan Gold Room Key -->
		</gd_items>
	</gd_rule>
</global_rules>